{% if wiki %}
    <co:wiki>{{ wiki.decode('utf_8') }}</co:wiki>
{% endif %}
{% if github %}
    <co:github>{{ github.decode('utf_8') }}</co:github>
{% endif %}
{% if sesame %}
    <co:sesame>{{ sesame.decode('utf_8') }}</co:sesame>
{% endif %}
{% if endpoint %}
    <co:endpoint>{{ endpoint.decode('utf_8') }}</co:endpoint>
{% endif %}