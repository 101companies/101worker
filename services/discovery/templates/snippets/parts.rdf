{% if fragments %}
    {% for fragment in fragments %}
		<co:fragment rdf:resource="{{ fragment.resource }}?format=rdf" co:name="{{ fragment.name }}" co:classifier="{{ fragment.classifier }}"/>
    {% endfor %}
{% endif %}

{% if folders %}
    {% for folder in folders %}
		<co:folder rdf:resource="{{ folder.resource }}?format=rdf" co:name="{{ folder.name }}" co:classifier="{{ folder.classifier }}"/>
    {% endfor %}
{% endif %}

{% if files %}
    {% for file in files %}
		<co:file rdf:resource="{{ file.resource }}?format=rdf" co:name="{{ file.name }}" co:classifier="{{ file.classifier }}"/>
    {% endfor %}
{% endif %}

{% if members %}
    {% for member in members %}
		<co:member rdf:resource="{{ member.resource }}?format=rdf" co:name="{{ member.name }}" co:classifier="{{ member.classifier }}"/>
    {% endfor %}
{% endif %}