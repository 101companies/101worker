{% extends "base.rdf" %}
{% block content %}
    {% include "snippets/general.rdf" %}

	<co:content>{{ content }}</co:content>

    {% for fragment in fragments %}
		<co:fragment rdf:resource="{{ fragment.resource }}?format=rdf" co:name="{{ fragment.name }}" co:classifier="{{ fragment.classifier }}"/>
    {% endfor %}
{% endblock %}