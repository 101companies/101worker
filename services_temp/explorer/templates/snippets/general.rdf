<co:name>{{ name.decode('utf_8') }}</co:name>
<co:namespace>{{ namespace.decode('utf-8') }}</co:namespace>
<co:classifier>{{ classifier.decode('utf_8') }}</co:classifier>
{% if geshi %}
	<co:geshi>{{ geshi }}</co:geshi>
{% endif %}
{% if language %}
    <co:language>{{ language }}</co:language>
{% endif %}