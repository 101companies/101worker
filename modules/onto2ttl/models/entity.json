 {
  "@context": {
     "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
     "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
     "owl": "http://www.w3.org/2002/07/owl#",
     "onto": "http://101companies.org/ontology#",
     "res": "http://101companies.org/resources#"
    },
  "@id": "onto:Entity",
  "@instance": "rdfs:Class",
  "comment": "Individuals in the scope of the ontology",
     "mapping" : [
      {
         "property": "instanceOf",
         "mapsTo": "rdfs:type"
       },
       {
         "property": "isA",
         "range": "rdfs:subclassOf"
       }
     ],
     "properties": [
       {
           "property": "onto:partOf",
           "range": "onto:Entity",
           "minCardinality": "0",
           "comment": "Whole-part relationship"
       },
       {
           "property": "onto:mentions",
           "range": "onto:Entity",
           "minCardinality": "0",
           "comment": "Nonspecific reference to an entity"
       },
       {
           "property": "onto:sameAs",
           "range": "rdfs:Literal",
           "minCardinality": "0",
           "comment": "Equivalence relative to external resource"
       },
       {
           "property": "onto:similarTo",
           "range": "rdfs:Literal",
           "minCardinality": "0",
           "comment": "Similarity relative to external resource"
       },
       {
           "property": "onto:linksTo",
           "range": "rdfs:Literal",
           "minCardinality": "0",
           "comment": "Non-specific link to external resource"
       },
       {
           "property": "onto:dependsOn",
           "range": "onto:Entity",
           "minCardinality": "0",
           "comment": "Dependence relationship"
       },
       {
           "property": "onto:carries",
           "range": "onto:Tag",
           "minCardinality": "0",
           "comment": "Tagging of entities"
       },
       {
           "property": "onto:memberOf",
           "range": "onto:Container",
           "minCardinality": "0",
           "comment": "Membership relationship"
       },
       {
           "property": "onto:moreComplexThan",
           "range": "onto:Entity",
           "minCardinality": "0",
           "comment": "Comparison of complexity"
       }
   ]
 }