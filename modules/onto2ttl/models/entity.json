 {
  "@context": {
     "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
     "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
     "owl": "http://www.w3.org/2002/07/owl#",
     "onto": "http://101companies.org/ontology#",
     "res": "http://101companies.org/resources#"
    },

  "@id": "onto:Entity",
  "@instance": "rdfs:Class",
     "mapping" : [
      {
         "property": "instanceOf",
         "mapsTo": "rdfs:type",
       },
       {
         "property": "isA",
         "range": "rdfs:subclassOf"
       },
     ],
     "properties": [
       {
           "property": "onto:partOf",
           "range": "onto:Entity",
           "minCardinality": "0",
           "comment": "Whole-part relationship"
       },
       {
           "property": "onto:mentions",
           "range": "onto:Entity",
           "minCardinality": "0",
           "comment": "Nonspecific reference to a concept"
       },
       {
           "property": "onto:sameAs",
           "range": "rdfs:Literal",
           "minCardinality": "0",
           "comment": ""
       },
       {
           "property": "onto:similarTo",
           "range": "rdfs:Literal",
           "minCardinality": "0"
       },
       {
           "property": "onto:linksTo",
           "range": "rdfs:Literal",
           "minCardinality": "0"
       },
       {
           "property": "onto:memberOf",
           "range": "onto:Vocabulary",
           "minCardinality": "0"
       },
       {
           "property": "onto:dependsOn",
           "range": "onto:Entity",
           "minCardinality": "0"
       },
       {
           "property": "onto:carries",
           "range": "onto:Tag",
           "minCardinality": "0"
       }
   ]
 }